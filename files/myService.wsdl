<definitions name="MyService" targetNamespace="https://sfwizard.herokuapp.com/wsdl/MyService.wsdl" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="https://sfwizard.herokuapp.com/wsdl/MyService.wsdl" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

   <message name="MyFunctionRequest">
      <part name="domain" type="xsd:string" />
   </message>
   <message name="MyFunctionResponse">
      <part name="ip" type="xsd:string" />
   </message>

   <portType name="MyPort">
      <operation name="MyFunction">
         <input message="tns:MyFunctionRequest" />
         <output message="tns:MyFunctionResponse" />
      </operation>
   </portType>

   <binding name="MyFunction_Binding" type="tns:MyPort">
      <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http" />
      <operation name="MyFunction">
         <soap:operation soapAction="MyFunction" />
         <input>
            <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="urn:examples:MyService" use="encoded" />
         </input>
         <output>
            <soap:body encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" namespace="urn:examples:MyService" use="encoded" />
         </output>
      </operation>
   </binding>

   <service name="MyService">
      <documentation>WSDL File for MyService</documentation>
      <port binding="tns:MyFunction_Binding" name="MyPort">
         <soap:address location="https://sfwizard.herokuapp.com/MyFunction/" />
      </port>
   </service>

</definitions>